@model MVCproj.Models.Loginn
@{
    ViewBag.Title = "Login_Pageload";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Login</h2>
<div>
    @using (Html.BeginForm("Login_Click", "LoginCon", FormMethod.Get))
    {
        <div>
            <label> User Name</label>
        </div>
        <div>
            @Html.TextBoxFor(m => m.uname)
            @Html.ValidationMessageFor(m => m.uname)
        </div>
        <div>
            <label>Password</label>
        </div>
        <div>
            @Html.TextBoxFor(m => m.password)
            @Html.ValidationMessageFor(m => m.password)
        </div>
        <div>
            <input type="submit" value="login" />
            @Html.DisplayFor(m => m.msg)

        </div>
    }
</div>
<div>
    @Html.ActionLink("User Registration", "User_insertPageload", "UserInsert")
</div>
<div>
    @Html.ActionLink("Company Registration", "insert_comp", "CompReg")
</div>

